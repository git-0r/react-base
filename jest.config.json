{
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "transformIgnorePatterns": [],
  "testEnvironment": "jsdom",
  "transform": {
    ".*\\.(tsx?|jsx?)$": [
      "@swc/jest",
      {
        "jsc": {
          "transform": {
            "react": {
              "runtime": "automatic"
            }
          }
        }
      }
    ]
  },
  "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"]
}
